.maps{background-color:#f6ebc6;outline:#aca389 1px solid;flex:1;display:block;height:100%;width:100%;padding:0}.embed{width:100%;height:100%}.linktoregister{color:#eaebff;border-radius:18px;text-decoration:none;width:fit-content;align-self:center;padding:calc(calc(max(min(1vw,4px),2px)) * 2);background-color:#3360f7;font-size:calc(max(min(6vw,32px),14px));transition:75ms}.linktoregister:hover{background-color:#40b746}.sponsors{display:flex;align-items:center;justify-content:center;gap:16px}.sponsors img{border-radius:6px;outline:1px solid #aca389}.sponsors .tbplc{outline:0;height:15vmax}.sponsors .chaska,.sponsors .funville{height:10vmax}