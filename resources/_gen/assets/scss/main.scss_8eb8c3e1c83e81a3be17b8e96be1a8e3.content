@import "https://fonts.googleapis.com/css2?family=Kanit:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Merriweather:ital,opsz,wght@0,18..144,300..900;1,18..144,300..900&display=swap";html,body{background-color:#ffe5e5;color:#272730;font-family:kanit,sans-serif;font-size:18px;margin:0}.navbar{background-color:#012fcc;font-family:merriweather;padding:4px;height:80px;box-sizing:border-box;font-weight:750;flex-direction:row;justify-content:space-between;display:flex;top:0;color:#eaebff;gap:4px;align-items:center;position:sticky;padding-left:16px;padding-right:16px}.navbar img{max-height:100%}.navbar div{height:100%;display:flex;align-items:center}.navbar a{color:#eaebff;border-radius:6px;text-decoration:none;padding:4px;padding-left:8px;padding-right:8px;background-color:#3771ff;transition:50ms}.navbar a:hover{background-color:#19c800}.navbar span{display:flex;text-wrap:nowrap;align-items:center;font-size:32px}.navbar span a{background-color:#012fcc}.navbar span a:hover{background-color:#012fcc}.navoptions{font-size:21.33333333px;font-family:kanit;font-weight:400;gap:8px}h1{font-family:merriweather}li::marker{color:#786b6a;font-size:24px}ol{gap:4px;display:flex;flex-direction:column}li{padding-bottom:4px;border-bottom:1px dotted #786b6a}li:last-of-type{padding-bottom:0;border-bottom:0}main{margin:auto;padding:8px;max-width:1200px;padding-left:32px;padding-right:32px}